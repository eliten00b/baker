SOURCE_URL="http://www.lua.org/ftp/lua-${VERSION}.tar.gz"
DEPENDENCIES="readline,ncurses"

PRE_CONFIGURE_COMMAND=
CONFIGURE_TOOL="true" # No configure tool
CONFIGURE_ARGS=
POST_CONFIGURE_COMMAND=

PRE_MAKE_COMMAND=
MAKE_TOOL=
MAKE_ARGS="linux MYLIBS=-lncurses"
POST_MAKE_COMMAND="make linux install INSTALL_TOP=${DESTDIR}${PREFIX}"

